<data-extraction-rules>
    <cloud-backup>
        <include domain="user" path="."/>
        <exclude domain="user" path="recordings/"/>
    </cloud-backup>
    <device-transfer>
        <include domain="user" path="."/>
        <include domain="user" path="recordings/"/>
    </device-transfer>
</data-extraction-rules>
